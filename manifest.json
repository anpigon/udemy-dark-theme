{
  "manifest_version": 3,
  "name": "Udemy Dark Theme",
  "version": "1.0",
  "description": "A simple dark theme for Udemy.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.udemy.com/*"],
      "css": ["dark-theme.css"]
    }
  ]
}
