{
  "manifest_version": 3,
  "name": "Udemy Dark Theme",
  "version": "1.1",
  "description": "A simple dark theme for Udemy.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.udemy.com/*"],
      "css": ["dark-theme.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
